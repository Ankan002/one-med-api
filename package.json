{
	"name": "one-med-api",
	"version": "0.0.1",
	"description": "This is the official API for One Med.",
	"main": "build/index.js",
	"repository": "https://github.com/Ankan002/one-med-api.git",
	"author": "Exponents",
	"license": "MIT",
	"scripts": {
		"start": "node build/index.js",
		"dev": "nodemon",
		"build": "yarn db-push && swc src -d build",
		"build-dev": "swc src -d build",
		"test": "jest --passWithNoTests",
		"test-dev": "jest --passWithNoTests --setupFiles dotenv/config",
		"db-push-dev": "npx prisma db push",
		"db-pull": "npx prisma db pull",
		"db-generate": "npx prisma generate",
		"db-push": "npx prisma db push --accept-data-loss",
		"prepare": "husky install",
		"lint": "eslint",
		"pre-commit": "yarn lint && yarn test-dev && yarn build-dev",
		"post-merge": "yarn install",
		"commit-msg": "npx --no-install commitlint --edit"
	},
	"dependencies": {
		"@prisma/client": "^4.11.0",
		"axios": "^1.3.4",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"date-fns": "^2.29.3",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.0",
		"jsonwebtoken": "^9.0.0",
		"morgan": "^1.10.0",
		"winston": "^3.8.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.42",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-fileupload": "^1.4.1",
		"@types/jest": "^29.5.0",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.15.10",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.7",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^5.0.2"
	}
}
